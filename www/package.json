    "html-webpack-plugin": "^5.6.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "embedded-engine": "file:/pkg"
  }
}